{
  "name": "prochat",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=.;NODE_ENV=production node ./bin/www",
    "coveragenyc": "nyc --check-coverage --lines 100  --timeout 15000 npm run coverage",
    "lint": "eslint *.js test/",
    "test": "NODE_PATH=.;NODE_ENV=test mocha --compilers js:babel-register",
    "test:win": "mocha test",
    "coverage": "rm -rf coverage/* && babel-node ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-register",
    "dev": "NODE_PATH=.;NODE_ENV=development nodemon ./bin/www --exec node",
    "compile": "rm -rf dist/* && babel ./src -d dist"
  },
  "dependencies": {
    "async": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "~1.19.0",
    "connect": "^3.7.0",
    "connect-mongo": "~3.2.0",
    "cookie": "~> 0.4.1",
    "cookie-parser": "~> 1.4.5",
    "cool-ascii-faces": "~> 1.3.4",
    "crypto": "^1.0.1",
    "debug": "^4.3.2",
    "ejs": "~> 3.1.6",
    "ejs-locals": "~> 1.0.2",
    "errorhandler": "~> 1.5.1",
    "express": "^4.17.1",
    "express-jquery": "~> 1.8.3",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.0",
    "morgan": "~1.10.0",
    "nconf": "~0.10.0",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.4",
    "winston": "~3.3.3"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "chai": "^4.3.4",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.0.0",
    "nodemon": "^1.18.6",
    "nyc": "^12.0.1",
    "prettier": "1.15.1",
    "standard": "^16.0.3",
    "supertest": "^3.0.0",
    "toboggan": "^1.0.0"
  }
}
